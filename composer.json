{
    "name": "babeuloula/nested-set",
    "description": "The nested set model is a technique for representing nested sets (also known as trees or hierarchies) in relational databases.",
    "type": "library",
    "license": "MIT",
    "keywords": ["nested", "nestedset", "symfony", "symfony-bride", "doctrine", "dbal", "databases", "relational", "hierarchies", "trees"],
    "homepage": "https://github.com/babeuloula/nested-set",
    "authors": [
        {
            "name": "BaBeuloula",
            "email": "info@babeuloula.fr",
            "homepage": "https://www.babeuloula.fr"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "babeuloula/phpcs": "^1.2",
        "phpstan/phpstan": "^0.12.8"
    },
    "autoload": {
        "psr-4": {
            "BaBeuloula\\NestedSet\\": "src/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
